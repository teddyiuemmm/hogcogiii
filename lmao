repeat task.wait() until game:IsLoaded()

local v1 = game:GetService("Players")
local v2 = game:GetService("TeleportService")
local v3 = v1.LocalPlayer

local function v4()
    local c = v3.Character or v3.CharacterAdded:Wait()
    return c:WaitForChild("HumanoidRootPart")
end

local function v5(s)
    return tonumber(s:match("^(%d+)"))
end

local function v6()
    local p = workspace:FindFirstChild("Pickups") or workspace:FindFirstChild("ActivePickups")
    if not p then return nil end
    local n, d = nil, math.huge
    local pos = v4().Position
    for _, v in ipairs(p:GetChildren()) do
        if v:IsA("BasePart") and v.Name == "Default" then
            local m = (v.Position - pos).Magnitude
            if m < d then
                d = m
                n = v
            end
        end
    end
    return n
end

local v7 = false

local function v8()
    v2:Teleport(game.PlaceId)
end

local function v9()
    local h = v4()
    for i = 1, math.random(2,4) do
        firetouchinterest(h, workspace.Utility.ObbyColliders.GameStart, 0)
        firetouchinterest(h, workspace.Utility.ObbyColliders.GameStart, 1)
        task.wait()
    end
end

local function v10()
    local h = v4()
    for i = 1, math.random(2,4) do
        firetouchinterest(h, workspace.Utility.ObbyColliders.GameStop.Collider, 0)
        firetouchinterest(h, workspace.Utility.ObbyColliders.GameStop.Collider, 1)
        task.wait()
    end
end

local function v11()
    if v7 then return end
    v7 = true

    v9()
    task.wait(0.2)

    local v12 = v3.PlayerGui:WaitForChild("PlayerUi")
        .Base.ObbyDisplays.TimerBase.Time

    while true do
        local t = v5(v12.Text)
        if t and t >= 148 then break end
        local n = v6()
        if n then
            v4().CFrame = n.CFrame + Vector3.new(0,1,0)
        end
        task.wait()
    end

    v10()
    v8()
    v7 = false
end

task.spawn(v11)

v3.CharacterAdded:Connect(function()
    task.wait(1)
    v7 = false
    task.spawn(v11)
end)
